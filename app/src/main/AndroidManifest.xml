<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    

    <application android:icon="@drawable/gitslogo" android:label="@string/app_name" >
        
        <activity android:name="com.coreprogrammers.musicplayer.GitsPlayerActivity"  android:label="@string/app_name" android:configChanges="keyboardHidden|orientation">
